########################################
# Authentication & API Configuration
########################################
api_base=http://192.168.86.33:9191
token_url=/api/accounts/token/
username=<YOUR USERNAME>
password=<YOUR PASSWORD>

########################################
# Streaming
########################################
stream_base_url=/proxy/ts/stream/
skip_stream_check=true
update_stream_link=false
only_updated_streams=false

########################################
# Output & Directories
########################################
output_root=/output
clean_output_dir=false

########################################
# Channel & File Filtering
########################################
process_movies_group=true
movies_groups=["Movies-*"]
process_tv_series_groups=true
tv_series_groups=["Series-*"]
process_groups_24_7=false
groups_24_7=["24/7*"]
remove_strings=[]

########################################
# Access Tokens (runtime-populated)
########################################
access=
refresh=

########################################
# TMDb Settings
########################################
tmdb_api_key=
tmdb_language=en-US
tmdb_image_size=original
tmdb_download_images=false
tmdb_create_not_found=false
check_tmdb_thresholds=false
minimum_year=1950
minimum_tmdb_rating=2
minimum_tmdb_votes=1
minimum_tmdb_popularity=5

########################################
# NFO File Options
########################################
write_nfo=false
write_nfo_only_if_not_exists=false

########################################
# OpenSubtitles Subtitle Support
########################################
opensubtitles_download=false
opensubtitles_app_name=<YOUR OPENSUBTITLE APP NAME>
opensubtitles_api_key=<YOUR OPENSUBTITLE API KEY>
opensubtitles_username=<YOUR OPENSUBTITLE USER NAME>
opensubtitles_password=<YOUR OPENSUBTITLE PASSWORD>

########################################
# Local Cache Paths
########################################
skip_cache_path=/skip_cache.json